{
    "name": "denniskupec/flysystem-backblaze",
    "type": "library",
    "description": "Backblaze adapter for the flysystem filesystem abstraction library",
    "keywords": ["flysystem", "league,", "filesystem", "api", "backblaze", "client", "adapter"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "league/flysystem": "^1.0",
        "cwhite92/b2-sdk-php": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7",
        "scrutinizer/ocular": "^1.3",
        "squizlabs/php_codesniffer": "^3.0.0RC2"
    },
    "autoload": {
        "psr-4": {
            "denniskupec\\Flysystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "denniskupec\\Flysystem\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
